# ENVIRONMENT
APP_ENV=production
APP_NAME=collider
APP_DEBUG=true
TIMEZONE=Europe/Moscow

# SCAN
SCAN_CACHEABLE=true

# DATABASE
DB_CONNECTION=default
DB_DRIVER=pgsql
DB_HOST=database
DB_PORT=5432
DB_PORTS=5432:5432
DB_DATABASE=collider_db
DB_USERNAME=collider_user
DB_PASSWORD=collider_secret
DB_CHARSET=utf8
DB_COLLATION=utf8_unicode_ci
DB_PREFIX=
DB_POOL_MIN_CONNECTIONS=50
DB_POOL_MAX_CONNECTIONS=10000
DB_POOL_CONNECT_TIMEOUT=30.0
DB_POOL_WAIT_TIMEOUT=10.0
DB_POOL_HEARTBEAT=60
DB_POOL_MAX_IDLE_TIME=300

# REDIS
REDIS_HOST=redis
REDIS_AUTH=
REDIS_PORT=6379
REDIS_DB=0
REDIS_POOL_MIN_CONNECTIONS=50
REDIS_POOL_MAX_CONNECTIONS=5000
REDIS_POOL_CONNECT_TIMEOUT=30.0
REDIS_POOL_WAIT_TIMEOUT=10.0
REDIS_POOL_HEARTBEAT=60
REDIS_POOL_MAX_IDLE_TIME=300

# CACHE
CACHE_DRIVER=redis
CACHE_PREFIX=
CACHE_DEFAULT_TTL=3600

# LOGGER
LOG_LEVEL=info
LOG_APP=default
LOG_TYPE=single

# Nginx
NGINX_PORT_HTTP=80
NGINX_PORT_HTTPS=443
