{
  "name": "bemyslavedarlin/collider",
  "type": "project",
  "description": "High-performance User Analytics API built with Hyperf",
  "keywords": [
    "php",
    "swoole",
    "framework",
    "hyperf",
    "collider",
    "analytics",
    "api",
    "coroutine",
    "performance"
  ],
  "license": "MIT",
  "require": {
    "php": ">=8.3",
    "ext-json": "*",
    "ext-openssl": "*",
    "ext-pcntl": "*",
    "ext-pdo": "*",
    "ext-pdo_pgsql": "*",
    "ext-redis": "*",
    "ext-swoole": ">=5.0",
    "fakerphp/faker": "^1.24",
    "hyperf/async-queue": "~3.1.0",
    "hyperf/cache": "~3.1.0",
    "hyperf/command": "~3.1.0",
    "hyperf/config": "~3.1.0",
    "hyperf/constants": "~3.1.0",
    "hyperf/contract": "~3.1.0",
    "hyperf/database": "^3.1",
    "hyperf/database-pgsql": "^3.1",
    "hyperf/db-connection": "^3.1",
    "hyperf/di": "~3.1.0",
    "hyperf/dispatcher": "~3.1.0",
    "hyperf/event": "~3.1.0",
    "hyperf/exception-handler": "~3.1.0",
    "hyperf/framework": "~3.1.0",
    "hyperf/guzzle": "~3.1.0",
    "hyperf/http-server": "~3.1.0",
    "hyperf/logger": "~3.1.0",
    "hyperf/memory": "~3.1.0",
    "hyperf/model-cache": "~3.1.0",
    "hyperf/paginator": "~3.1.0",
    "hyperf/pool": "~3.1.0",
    "hyperf/process": "~3.1.0",
    "hyperf/redis": "~3.1.0",
    "hyperf/server": "~3.1.0",
    "hyperf/utils": "~3.1.0",
    "hyperf/validation": "~3.1.0",
    "hyperf/view": "~3.1.0",
    "hyperf/watcher": "~3.1.0",
    "nesbot/carbon": "^2.73",
    "psr/simple-cache": "^3.0",
    "ramsey/uuid": "^4.8"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.0",
    "hyperf/devtool": "~3.1.0",
    "hyperf/testing": "~3.1.0",
    "mockery/mockery": "^1.0",
    "phpstan/phpstan": "^1.0",
    "swoole/ide-helper": "^5.0",
    "roave/security-advisories": "dev-latest"
  },
  "suggest": {
    "ext-openssl": "Required to use HTTPS.",
    "ext-json": "Required to use JSON.",
    "ext-pdo": "Required to use PgSQL Client.",
    "ext-pdo_pgsql": "Required to use PgSQL Client.",
    "ext-redis": "Required to use Redis Client."
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    },
    "files": []
  },
  "autoload-dev": {
    "psr-4": {
      "HyperfTest\\": "./test/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true
  },
  "extra": {},
  "scripts": {
    "test": "co-phpunit --prepend test/bootstrap.php --colors=always",
    "cs-fix": "php-cs-fixer fix $1",
    "analyse": "phpstan analyse --memory-limit 1G",
    "start": [
      "Composer\\Config::disableProcessTimeout",
      "php ./bin/hyperf.php start"
    ]
  }
}
